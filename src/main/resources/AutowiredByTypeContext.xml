<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="employee" class="com.example.autowired.type.Employee">
        <property name="empName" value="Sneha" />
        <property name="role" value="Software dev" />
        <property name="age" value="23" />
        <property name="salary" value="50000.00" />
    </bean>

    <bean id="manager" class="com.example.autowired.type.Manager" autowire="byType">
        <property name="managerName" value="Dinesh" />
    </bean>

</beans>