<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="employee" class="com.example.autowired.constructor.Employee">
        <property name="empName" value="Pranav" />
        <property name="role" value="Software dev" />
        <property name="age" value="22" />
        <property name="salary" value="20000.00" />
    </bean>

    <bean id="manager" class="com.example.autowired.constructor.Manager" autowire="constructor">
        <property name="managerName" value="Siva" />
    </bean>

</beans>